        <rdfs:subClassOf rdf:resource="&www;TraditionalChineseMedicineTreatment"/>
        <rdfs:comment xml:lang="en">A general rule that should be followed in treating disease.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.example.com/Therapy -->

    <owl:Class rdf:about="&www;Therapy">
        <rdfs:label xml:lang="en">therapy</rdfs:label>
        <rdfs:label xml:lang="zh">治法</rdfs:label>
        <rdfs:subClassOf rdf:resource="&www;TraditionalChineseMedicineTreatment"/>
        <rdfs:comment xml:lang="en">Therapy, or treatment, is the attempted remediation of a health problem. Any specific intervention derived from a principle of treatment, e.g. diaphoresis, emesis, purgation, mediation, warming, clearing, tonification and resolution.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.example.com/Toxin -->

    <owl:Class rdf:about="&www;Toxin">
