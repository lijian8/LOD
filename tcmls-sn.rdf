<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY www "http://www.example.com/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY xml "http://www.w3.org/XML/1998/namespace" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.example.com/"
     xml:base="http://www.example.com/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:www="http://www.example.com/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace">
    <owl:Ontology rdf:about="http://www.example.com/">
        <rdfs:comment xml:lang="en">An example of OWL ontology for Traditional Chinese Medicine domain.</rdfs:comment>
        <rdfs:comment xml:lang="zh">中医药领域本体示例。</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&rdfs;seeAlso"/>
    <owl:AnnotationProperty rdf:about="&rdfs;isDefinedBy"/>
    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral -->

    <rdfs:Datatype rdf:about="&rdf;PlainLiteral"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.example.com/adjacent_to -->

    <owl:ObjectProperty rdf:about="&www;adjacent_to">
        <rdfs:label xml:lang="en">adjacent to</rdfs:label>
        <rdfs:label xml:lang="zh">与…相邻</rdfs:label>
        <rdfs:comment xml:lang="en">Close to, near or abutting another physical unit with no other structure of the same kind intervening. This includes adjoins, abuts, is contiguous to, is juxtaposed, and is close to.</rdfs:comment>
        <rdfs:comment xml:lang="zh">与另一个物理单位紧密地相连的，邻接的，没有其他的同样的结构介入其中。包括毗邻，邻接，邻近的，并列的，紧密联结的。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;spatially_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/affects -->

    <owl:ObjectProperty rdf:about="&www;affects">
        <rdfs:label xml:lang="en">affects</rdfs:label>
        <rdfs:label xml:lang="zh">影响</rdfs:label>
        <rdfs:comment xml:lang="en">Produces a direct effect on. Implied here is the altering or influencing of an existing condition, state, situation, or entity. This includes has a role in, alters, influences, predisposes, catalyzes, stimulates, regulates, depresses, impedes, enhances, contributes to, leads to, and modifies.</rdfs:comment>
        <rdfs:comment xml:lang="zh">改变或影响现有的条件、状态、情形、实体产生一种直接的效果。包括起作用，改变，影响，预先安排，刺激、促进，管理、控制、调节，压下，阻止，提高、增强，起作用、有助于、促成，引导、致使，更改、修改等。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;functionally_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/analyzes -->

    <owl:ObjectProperty rdf:about="&www;analyzes">
        <rdfs:label xml:lang="en">analyzes</rdfs:label>
        <rdfs:label xml:lang="zh">分析</rdfs:label>
        <rdfs:comment xml:lang="en">Studies or examines using established quantitative or qualitative methods.</rdfs:comment>
        <rdfs:comment xml:lang="zh">用确定的定量的或定型的方法研究或检查。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;conceptually_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/assesses_effect_of -->

    <owl:ObjectProperty rdf:about="&www;assesses_effect_of">
        <rdfs:label xml:lang="en">assesses effect of</rdfs:label>
        <rdfs:label xml:lang="zh">分析评估…的影响</rdfs:label>
        <rdfs:comment xml:lang="en">Analyzes the influence or consequences of the function or action of.</rdfs:comment>
        <rdfs:comment xml:lang="zh">分析功能或行为的影响或结果。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;analyzes"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/associated_with -->

    <owl:ObjectProperty rdf:about="&www;associated_with">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">associated with</rdfs:label>
        <rdfs:label xml:lang="zh">相关关系链</rdfs:label>
        <rdfs:comment xml:lang="en">has a significant or salient relationship to</rdfs:comment>
        <rdfs:comment xml:lang="zh">有重要的或显著的关系</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/branch_of -->

    <owl:ObjectProperty rdf:about="&www;branch_of">
        <rdfs:label xml:lang="en">branch of</rdfs:label>
        <rdfs:label xml:lang="zh">分支</rdfs:label>
        <rdfs:comment xml:lang="en">Arises from the division of. For example, the arborization of arteries.</rdfs:comment>
        <rdfs:comment xml:lang="zh">出现于分开的部分。例如，动脉的分支。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;physically_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/brings_about -->

    <owl:ObjectProperty rdf:about="&www;brings_about">
        <rdfs:label xml:lang="en">brings about</rdfs:label>
        <rdfs:label xml:lang="zh">带来-造成</rdfs:label>
        <rdfs:comment xml:lang="en">Acts on or influences an entity.</rdfs:comment>
        <rdfs:comment xml:lang="zh">作用或影响一个实体</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;functionally_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/carries_out -->

    <owl:ObjectProperty rdf:about="&www;carries_out">
        <rdfs:label xml:lang="en">carries out</rdfs:label>
        <rdfs:label xml:lang="zh">实施</rdfs:label>
        <rdfs:comment xml:lang="en">Executes a function or performs a procedure or activity. This includes transacts, operates on, handles, and executes.</rdfs:comment>
        <rdfs:comment xml:lang="zh">执行一项功能或完成一项手续或活动。包括处理，操作、运转，操纵、运用，实行、执行等。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;performs"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/causes -->

    <owl:ObjectProperty rdf:about="&www;causes">
        <rdfs:label xml:lang="en">causes</rdfs:label>
        <rdfs:label xml:lang="zh">引起</rdfs:label>
        <rdfs:comment xml:lang="en">Brings about a condition or an effect. Implied here is that an agent, such as for example, a pharmacologic substance or an organism, has brought about the effect. This includes induces, effects, evokes, and etiology.</rdfs:comment>
        <rdfs:comment xml:lang="zh">引起一种状态或结果。这里所指的是一种原因，例如，一种药物或有机体产生的结果。包括引起，导致，唤起，病变的原因。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;brings_about"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/co-occurs_with -->

    <owl:ObjectProperty rdf:about="&www;co-occurs_with">
        <rdfs:label xml:lang="en">co-occurs with</rdfs:label>
        <rdfs:label xml:lang="zh">与…同时发生</rdfs:label>
        <rdfs:comment xml:lang="en">Occurs at the same time as, together with, or jointly. This includes is co-incident with, is concurrent with, is contemporaneous with, accompanies, coexists with, and is concomitant with.</rdfs:comment>
        <rdfs:comment xml:lang="zh">在同一时间一起或者共同地、连带地发生。包括同时发生的，并发的，同时期发生的，伴发的，共存的，伴随产生的等。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;temporally_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/complicates -->

    <owl:ObjectProperty rdf:about="&www;complicates">
        <rdfs:label xml:lang="en">complicates</rdfs:label>
        <rdfs:label xml:lang="zh">并发</rdfs:label>
        <rdfs:comment xml:lang="en">Causes to become more severe or complex or results in adverse effects.</rdfs:comment>
        <rdfs:comment xml:lang="zh">引起变得更严重或更复杂或产生一种不利的结果。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;affects"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/conceptual_part_of -->

    <owl:ObjectProperty rdf:about="&www;conceptual_part_of">
        <rdfs:label xml:lang="en">conceptual part of</rdfs:label>
        <rdfs:label xml:lang="zh">…的概念部分</rdfs:label>
        <rdfs:comment xml:lang="en">Conceptually a portion, division, or component of some larger whole.</rdfs:comment>
        <rdfs:comment xml:lang="zh">概念的一部分，或某个较大整体的组成成分。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;conceptually_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/conceptually_related_to -->

    <owl:ObjectProperty rdf:about="&www;conceptually_related_to">
        <rdfs:label xml:lang="en">conceptually related to</rdfs:label>
        <rdfs:label xml:lang="zh">概念上相关</rdfs:label>
        <rdfs:comment xml:lang="en">Related by some abstract concept, thought, or idea.</rdfs:comment>
        <rdfs:comment xml:lang="zh">被某些理论性的概念、思想和想法相关。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;associated_with"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/connected_to -->

    <owl:ObjectProperty rdf:about="&www;connected_to">
        <rdfs:label xml:lang="en">connected to</rdfs:label>
        <rdfs:label xml:lang="zh">与…相连</rdfs:label>
        <rdfs:comment xml:lang="en">Directly attached to another physical unit as tendons are connected to muscles. This includes attached to and anchored to.</rdfs:comment>
        <rdfs:comment xml:lang="zh">直接地附着在另一个物理单元上，比如肌腱与肌肉相连。包括附着在…上，固定在…上。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;physically_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/consists_of -->

    <owl:ObjectProperty rdf:about="&www;consists_of">
        <rdfs:label xml:lang="en">consists of</rdfs:label>
        <rdfs:label xml:lang="zh">由…组成</rdfs:label>
        <rdfs:comment xml:lang="en">Is structurally made up of in whole or in part of some material or matter. This includes composed of, made of, and formed of.</rdfs:comment>
        <rdfs:comment xml:lang="zh">一个整体的结构上的组成部分或者某种材料或物质的构成部分。包括由…组成，由…构成，由…形成等。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;physically_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/contains -->

    <owl:ObjectProperty rdf:about="&www;contains">
        <rdfs:label xml:lang="en">contains</rdfs:label>
        <rdfs:label xml:lang="zh">包含</rdfs:label>
        <rdfs:comment xml:lang="en">Holds or is the receptacle for fluids or other substances. This includes is filled with, holds, and is occupied by.</rdfs:comment>
        <rdfs:comment xml:lang="zh">包含</rdfs:comment>
        <rdfs:comment xml:lang="zh">可容纳或者是一个贮藏液体或其他物质的容器。包括被…充满，可容纳，被…占用等。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;physically_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/degree_of -->

    <owl:ObjectProperty rdf:about="&www;degree_of">
        <rdfs:label xml:lang="en">degree of</rdfs:label>
        <rdfs:label xml:lang="zh">…的程度-等级</rdfs:label>
        <rdfs:comment xml:lang="en">The relative intensity of a process or the relative intensity or amount of a quality or attribute.</rdfs:comment>
        <rdfs:comment xml:lang="zh">一个过程相对的强度，一种属性或性质的相对强度或数量。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;conceptually_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/derivative_of -->

    <owl:ObjectProperty rdf:about="&www;derivative_of">
        <rdfs:label xml:lang="en">derivative of</rdfs:label>
        <rdfs:label xml:lang="zh">衍生的</rdfs:label>
        <rdfs:comment xml:lang="en">In chemistry, a substance structurally related to another or that can be made from the other substance. This is used only for structural relationships. This does not include functional relationships such as metabolite of, by product of, nor analog of.</rdfs:comment>
        <rdfs:comment xml:lang="zh">化学上，一种物质与另一种物质在结构上相关，或者能够被其他的物质所产生。这仅仅指结构上的关联。不包括功能上的关联比如.…的代谢物，.…的产物，.…的类似物。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;conceptually_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/developmental_form_of -->

    <owl:ObjectProperty rdf:about="&www;developmental_form_of">
        <rdfs:label xml:lang="en">developmental form of</rdfs:label>
        <rdfs:label xml:lang="zh">…的发育阶段</rdfs:label>
        <rdfs:comment xml:lang="en">An earlier stage in the individual maturation of.</rdfs:comment>
        <rdfs:comment xml:lang="zh">个体成熟过程中的早期阶段。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;conceptually_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/diagnoses -->

    <owl:ObjectProperty rdf:about="&www;diagnoses">
        <rdfs:label xml:lang="en">diagnoses</rdfs:label>
        <rdfs:label xml:lang="zh">诊断</rdfs:label>
        <rdfs:comment xml:lang="en">Distinguishes or identifies the nature or characteristics of.</rdfs:comment>
        <rdfs:comment xml:lang="zh">区别或鉴别特性或特征。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;conceptually_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/disrupts -->

    <owl:ObjectProperty rdf:about="&www;disrupts">
        <rdfs:label xml:lang="en">disrupts</rdfs:label>
        <rdfs:label xml:lang="zh">干扰</rdfs:label>
        <rdfs:comment xml:lang="en">Alters or influences an already existing condition, state, or situation. Produces a negative effect on.</rdfs:comment>
        <rdfs:comment xml:lang="zh">改变或影响一个已经存在的条件、状态或情形，产生一种消极的影响。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;affects"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/engenders -->

    <owl:ObjectProperty rdf:about="&www;engenders">
        <rdfs:label xml:lang="en">engenders</rdfs:label>
        <rdfs:label xml:lang="zh">生</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&www;affects"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/evaluation_of -->

    <owl:ObjectProperty rdf:about="&www;evaluation_of">
        <rdfs:label xml:lang="en">evaluation of</rdfs:label>
        <rdfs:label xml:lang="zh">…的评价</rdfs:label>
        <rdfs:comment xml:lang="en">Judgment of the value or degree of some attribute or process.</rdfs:comment>
        <rdfs:comment xml:lang="zh">判断某种属性或过程的价值或程度。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;conceptually_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/exhibits -->

    <owl:ObjectProperty rdf:about="&www;exhibits">
        <rdfs:label xml:lang="en">exhibits</rdfs:label>
        <rdfs:label xml:lang="zh">展示</rdfs:label>
        <rdfs:comment xml:lang="en">Shows or demonstrates.</rdfs:comment>
        <rdfs:comment xml:lang="zh">表示、出示、证明等。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;performs"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/functionally_related_to -->

    <owl:ObjectProperty rdf:about="&www;functionally_related_to">
        <rdfs:label xml:lang="en">functionally related to</rdfs:label>
        <rdfs:label xml:lang="zh">功能上相关</rdfs:label>
        <rdfs:comment xml:lang="en">Related by the carrying out of some function or activity.</rdfs:comment>
        <rdfs:comment xml:lang="zh">由于某些功能或者活动而相关。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;associated_with"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/indicates -->

    <owl:ObjectProperty rdf:about="&www;indicates">
        <rdfs:label xml:lang="en">indicates</rdfs:label>
        <rdfs:label xml:lang="zh">证实</rdfs:label>
        <rdfs:comment xml:lang="en">Gives evidence for the presence at some time of an entity or process.</rdfs:comment>
        <rdfs:comment xml:lang="zh">为一个实体或过程在某一时间的存在提供证据。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;functionally_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/ingredient_of -->

    <owl:ObjectProperty rdf:about="&www;ingredient_of">
        <rdfs:label xml:lang="en">ingredient of</rdfs:label>
        <rdfs:label xml:lang="zh">…成份</rdfs:label>
        <rdfs:comment xml:lang="en">Is a component of, as in a constituent of a preparation.</rdfs:comment>
        <rdfs:comment xml:lang="zh">组成成分或构成成分。指一个部分的要素、成分。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;physically_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/inter_exterior_and_interior_with -->

    <owl:ObjectProperty rdf:about="&www;inter_exterior_and_interior_with">
        <rdfs:label xml:lang="en">inter exterior and interior with</rdfs:label>
        <rdfs:label xml:lang="zh">与…相表里</rdfs:label>
        <rdfs:comment xml:lang="en"></rdfs:comment>
        <rdfs:comment xml:lang="zh">脏腑以及经络之间的阴阳表里关系，反映的是脏腑经络功能上的相互影响。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;affects"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/interacts_with -->

    <owl:ObjectProperty rdf:about="&www;interacts_with">
        <rdfs:label xml:lang="en">interacts with</rdfs:label>
        <rdfs:label xml:lang="zh">与…相互作用</rdfs:label>
        <rdfs:comment xml:lang="en">Acts, functions, or operates together with</rdfs:comment>
        <rdfs:comment xml:lang="zh">与…一起产生效果，起作用</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;affects"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/interconnects -->

    <owl:ObjectProperty rdf:about="&www;interconnects">
        <rdfs:label xml:lang="en">interconnects</rdfs:label>
        <rdfs:label xml:lang="zh">使…相互联系</rdfs:label>
        <rdfs:comment xml:lang="en">Serves to link or join together two or more other physical units. This includes joins, links, conjoins, articulates, separates, and bridges.</rdfs:comment>
        <rdfs:comment xml:lang="zh">使两个或多个物理单元连接或结合在一起。包括结合，连接，连结，用关节连接，分离？桥梁连接等。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;physically_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/isa -->

    <owl:ObjectProperty rdf:about="&www;isa">
        <rdfs:label xml:lang="en">isa</rdfs:label>
        <rdfs:label xml:lang="en">等级关系链</rdfs:label>
        <rdfs:label xml:lang="zh">等级关系链</rdfs:label>
        <rdfs:comment xml:lang="en"></rdfs:comment>
        <rdfs:comment xml:lang="zh"></rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/issue_in -->

    <owl:ObjectProperty rdf:about="&www;issue_in">
        <rdfs:label xml:lang="en">issue in</rdfs:label>
        <rdfs:label xml:lang="zh">…之中的焦点问题</rdfs:label>
        <rdfs:comment xml:lang="en">Is an issue in or a point of discussion, study, debate, or dispute.</rdfs:comment>
        <rdfs:comment xml:lang="zh">讨论、研究、争论、辩论中的要点、关键。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;conceptually_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/location_of -->

    <owl:ObjectProperty rdf:about="&www;location_of">
        <rdfs:label xml:lang="en">location of</rdfs:label>
        <rdfs:label xml:lang="zh">位于…</rdfs:label>
        <rdfs:comment xml:lang="en">The position, site, or region of an entity or the site of a process.</rdfs:comment>
        <rdfs:comment xml:lang="zh">位置、场所，一个实体的位置或一个过程的场所。</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;spatially_related_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/manages -->

    <owl:ObjectProperty rdf:about="&www;manages">
        <rdfs:label xml:lang="en">manages</rdfs:label>
        <rdfs:label xml:lang="zh">管理</rdfs:label>
        <rdfs:comment xml:lang="en">Administers, or contributes to the care of an individual or group of individuals.</rdfs:comment>
        <rdfs:comment xml:lang="zh">管理，有助于照顾个体或由个体组成的团体</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&www;affects"/>
    </owl:ObjectProperty>
    


    <!-- http://www.example.com/manifestation_of -->

    <owl:ObjectProperty rdf:about="&www;manifestation_of">
        <rdfs:label xml:lang="en">manifestation of</rdfs:label>
        <rdfs:label xml:lang="zh">现象表达</rdfs:label>
        <rdfs:comment xml:lang="en">That part of a phenomenon which is directly observable or concretely or visibly expressed, or which gives evidence to the underlying process. This includes expression of, display of, and exhibition of.</rdfs:comment>
